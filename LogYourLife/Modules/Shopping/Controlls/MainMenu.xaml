<UserControl x:Class="LogYourLife.Modules.Shopping.Controlls.MainMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LogYourLife.Modules.Shopping.Controlls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Width="150" Orientation="Vertical" HorizontalAlignment="Left">
        <Button x:Name="btnNewShopping" Click="btnNewShopping_Click" >newShopping</Button>
        <Label></Label>
        <Button x:Name="btnNewStore" Click="btnNewStore_Click" >newStore</Button>
        <Button x:Name="btnNewArticle" Click="btnNewArticle_Click" >newArticle</Button>
        <Button x:Name="btnNewBrand" Click="btnNewBrand_Click">newBrand</Button>
        <Button x:Name="btnNewMeasure" Click="btnNewMeasure_Click">newMeasure</Button>
        <Label></Label>
        <Button x:Name="btnShowArticles" Click="btnShowArticles_Click">showArticles</Button>
        <Button x:Name="btnShowStores" Click="btnShowStores_Click" >showStores</Button>
        <Button x:Name="btnShowBrands" Click="btnShowBrands_Click" >showBrands</Button>
        <Button x:Name="btnShowMeasures" Click="btnShowMeasures_Click">showMeasures</Button>
    </StackPanel>
</UserControl>
